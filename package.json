{
  "name": "bak-open-ledger",
  "version": "0.0.0",
  "description": "BAK Open Compatibility Ledger",
  "main": "index.js",
  "scripts": {
    "cli": "ts-node -r tsconfig-paths/register src/Presentation/CLI/Program.ts",
    "cli:debug": "node --inspect-brk -r ts-node/register -r tsconfig-paths/register src/Presentation/CLI/Program.ts",
    "test": "jest",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "db:migrate": "prisma migrate dev",
    "db:seed": "ts-node prisma/seed.ts",
    "db:studio": "prisma studio",
    "test:cov": "jest --coverage",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint src tests --ext .ts",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.9",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "jest": "^30.2.0",
    "jest-mock-extended": "^4.0.0",
    "prettier": "^3.8.0",
    "prisma": "^5.22.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "dotenv": "^17.2.3",
    "dotnet-node-core": "github:fabioferreccio/dotnet-node-core",
    "jose": "^6.1.3"
  },
  "overrides": {
    "diff": "^8.0.3"
  }
}
